<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="plan_picking_template_view_tree" model="ir.ui.view">
            <field name="name">plan.picking.template.tree</field>
            <field name="model">project.plan.pickings</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="description"/>
                    <field name="creation_date"/>
                </tree>
            </field>
        </record>

        <record id="plan_picking_template_view_form" model="ir.ui.view">
            <field name="name">plan.picking.template.view.from</field>
            <field name="model">project.plan.pickings</field>
            <field name="arch" type="xml">
                <form string="Project plan picking template">
                    <sheet>
                        <div class="oe_title">
                            <label for="name"/>
                            <h1><field name="name" class="w-100" required="1"/></h1>
                            <group>
                                <group>
                                    <field name="creator_id"/>
                                    
                                </group>
                                <group>
                                    <field name="creation_date"/>
                                </group>
                            </group>
                        </div>
                        <notebook>
                            <page name="description" string="Description">
                                <field name="description"/>
                            </page>
                            <page name="picking_lines" string="Pickings">
                                <field name="project_picking_lines">
                                    <tree editable="editable_bottom">
                                        <field name="product_id"/>
                                        <field name="quantity"/>
                                        <field name="location_id"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>