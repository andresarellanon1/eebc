<odoo>
    <record id="inherit_sale_order_form_view" model="ir.ui.view">
        <field name="name">inherit.sale.order.form.view</field>
        <field name="model">sale.order.line</field>
        <field name="inherit_id" ref="sale.view_order_line_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_uom_qty']" position="before">
                <field name="complete_description" editable="True"/>
            </xpath>
        </field>
    </record>
</odoo>