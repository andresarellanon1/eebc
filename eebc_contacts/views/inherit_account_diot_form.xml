<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="inherit_account_diot_form" model="ir.ui.view">
        <field name="name">inherit.account.diot.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="l10n_mx_reports.res_partner_account_diot_form_oml"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='diot_information']" position="after">
                <group string="IMPUESTOS DE PROVEEDORES" name="supplier_taxes">
                    <field name="supplier_taxes" widget="many2many_tags"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>